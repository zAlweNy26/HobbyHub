{
  "name": "hobbies-tracker",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Track your hobbies all in one single app!",
  "author": "zAlweNy26",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zAlweNy26/hobbies_tracker.git"
  },
  "homepage": "https://github.com/zAlweNy26/hobbies_tracker#readme",
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue",
    "hobbies",
    "tracker",
    "ts",
    "typescript"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "check": "vue-tsc --noEmit",
    "preview": "vite preview",
    "build": "npm run check && vite build",
    "mac": "npm run build && electron-builder --mac",
    "windows": "npm run build && electron-builder --win",
    "portable": "npm run build && electron-builder --win portable"
  },
  "devDependencies": {
    "@types/igdb-api-node": "^5.0.0",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.4",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@floating-ui/vue": "^0.2.0",
    "@headlessui/tailwindcss": "^0.1.2",
    "@headlessui/vue": "^1.7.7",
    "@iconify/vue": "^4.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "animate.css": "^4.1.1",
    "apicalypse": "^0.2.0",
    "daisyui": "^2.49.0",
    "howlongtobeat": "^1.7.0",
    "igdb-api-node": "^5.0.2",
    "imdb-api": "^4.4.1",
    "json-schema-to-typescript": "^11.0.3",
    "pinia": "^2.0.29",
    "tailwind-children": "^0.5.0",
    "tailwindcss-gradients": "^3.0.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vue-router": "^4.1.6"
  }
}
